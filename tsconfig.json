{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ]
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}